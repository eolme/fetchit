{
  "name": "@eolme/fetchit",
  "description": "Promise based HTTP client with compatible API.",
  "version": "1.0.2",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .json --ext .md",
    "pretest": "npm run lint",
    "test": "echo \"No tests\"",
    "precommit": "lint-staged",
    "postcommit": "git update-index --again",
    "preversion": "npm test",
    "postversion": "git push && git push --tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --debug --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eolme/fetchit.git"
  },
  "keywords": [
    "fetch",
    "axios"
  ],
  "author": "Petrov Anton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eolme/fetchit/issues"
  },
  "homepage": "https://github.com/eolme/fetchit#readme",
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-markdown": "^1.0.0-rc.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^1.1.4",
    "lint-staged": "^8.0.5"
  }
}
